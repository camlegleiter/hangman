{
  "name": "hangman",
  "version": "1.0.0",
  "description": "An Express application and API for playing Hangman",
  "main": "index.js",
  "repository": "git@github.com:camlegleiter/hangman.git",
  "author": "Cameron Legleiter <camlegleiter@gmail.com>",
  "license": "MIT",
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/ test/",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "start": "node --require babel-register index.js",
    "test": "NODE_ENV=test npm run test:spec && npm run test:http",
    "test:coverage": "NODE_ENV=test nyc --require babel-register --reporter=text --reporter=text-summary ./node_modules/mocha/bin/_mocha --require test/.setup.js test/**/*.js",
    "test:spec": "NODE_ENV=test mocha --require babel-register --require test/.setup.js test/**/*.spec.js",
    "test:http": "NODE_ENV=test mocha --require babel-register --require test/.setup.js test/**/*.http.js"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.22.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.17.0",
    "bunyan": "^1.8.8",
    "config": "^1.25.1",
    "express": "^4.14.1",
    "helmet": "^3.4.0",
    "js-yaml": "^3.8.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.21",
    "log4js": "^6.4.0",
    "moment": "^2.29.2",
    "morgan": "^1.8.0",
    "noop": "^0.2.2",
    "pg": "^6.1.2",
    "pug": "^3.0.1",
    "rotating-file-stream": "^1.1.7",
    "standard-http-error": "^2.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.1.8",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cheerio": "^0.22.0",
    "coveralls": "^3.0.5",
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^10.1.0",
    "nyc": "^14.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "supertest": "^3.0.0"
  }
}
